    {
        "id": "59b284a3.6cebcc",
        "type": "function",
        "z": "d671377f.d4d748",
        "name": "Build a piechart",
        "func": "var Tweets = \"\\\"timestamp\\\",\\\"tweet\\\",\\\"sentiment\\\"\\n\";\nvar pos = 0;\nvar neg = 0;\nvar neutral =0;\n\nfor( i=0;i<msg.payload.length;i++) {\n    if( typeof ( msg.payload[i].sentiment ) != \"undefined\") {    \n        if( msg.payload[i].sentiment > 0 ) {\n            pos++;\n        } else if( msg.payload[i].sentiment < 0 ) {\n            neg++;\n        } else {\n            neutral++;\n        }\n    }\n}\n\nvar pie={ \"labels\" : [\"Positive\",\"Negative\",\"Neutral\"],\n          \"series\" : [\"Positive\",\"Negative\",\"Neutral\"],\n          \"data\"   : [ [pos, neg, neutral] ]\n        };\nmsg.payload = [pie];\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "x": 1300,
        "y": 620,
